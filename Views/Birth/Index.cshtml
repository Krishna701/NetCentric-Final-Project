                function Populate(ddlName) {
                    var dataType = jQuery('#' + ddlName).attr('dataType');
                    if (!dataType) {
                        return;
                    }
                    else // data dropdown detected, hence PROCEED
                    {
                        var myId = jQuery('#' + ddlName).attr('id');
